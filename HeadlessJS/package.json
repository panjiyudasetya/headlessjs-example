		"babel-preset-react-native": "1.9.2",
    	"concurrently": "^3.4.0",
		"jest": "20.0.4",
		"react-test-renderer": "16.0.0-alpha.6",
		"rimraf": "^2.6.1",
		"tslint": "^5.2.0",
		"typescript": "^2.3.2"
	},
